   <servers>
      <server>
         <id>central</id>
         <username>${env.MAVEN_REPO_USER}</username>
         <password>${env.MAVEN_REPO_PASS}</password>
      </server>
      <server>
         <id>snapshots</id>
         <username>${env.MAVEN_REPO_USER}</username>
         <password>${env.MAVEN_REPO_PASS}</password>
      </server>
      <server>
         <id>ossrh</id>
         <username>${env.JIRA_USER}</username>
         <password>${env.JIRA_PASS}</password>
      </server>
